        <script type="text/javascript">
        $(document).ready(function () {
            $(".logout").click(function (event) {
                event.preventDefault();
                var sweet = $(this);
                swal({
                    title: "Are you sure?",
                    text: "You are logout",
                    icon: "warning",
                    buttons: true,
                    dangerMode: true,
                }).then((willDelete) => {
                    if (willDelete) {
                        $.get($(this).attr('href'), function (response) {
                            swal("Poof! Your account is logout!", {
                                icon: "success",
                            }).then(function () {
                                window.location.reload();
                            });
                        });
                    } else {
                        swal("An error occurred!");
                    }
                });
            });
        });

 </script>
        
   
